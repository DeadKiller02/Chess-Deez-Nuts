{
  "name": "chess-deez-nuts",
  "version": "0.0.1",
  "description": "A simple chess interface with a machine AI hardcoded with my fellow collaborators",
  "main": "index.js",
  "repository": "https://github.com/Ragudos/Chess-Deez-Nuts.git",
  "author": "Aaron Ragudos <workemailaaronragudos@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "www",
    "algorithm",
    "core",
    "__tests__"
  ],
  "scripts": {
    "format": "prettier --write [\"www/src/**/*.{ts,js,html}\",\"algorithm/src/**/*.ts\", \"core/src/**/*.ts\", \"__tests__/**/*.ts\"]",
    "test": "yarn workspace @chess-deez-nuts/__tests__ run test",
    "build": "yarn workspaces foreach --all -pt build",
    "dev": "yarn workspaces foreach --all -pt dev"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.2",
    "typescript": "^5.3.3"
  }
}
